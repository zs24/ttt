<?xml version="1.0" encoding="UTF-8" ?>
<parms>
   <!--  pump_rac_ge_monkey.com -->
   <!--  drive file for raclopride in the sc2 -->
   <!--  -->
   <!--   diam=="19.13"        ! B.D. -->
   <!--   rmax=="13.6" !max speeed for this diameter -->
   <!-- syringe_name=="20 cc BD  syringe" -->
   <!-- kbol=="60" -->
   <!--  changed to 120 min on 10-8-02 -->
   <!--  Changed to 150 minutes 11/23/2002 -->
   <!--  10-8-02 6.2 mCi total = 5 mCi as bolus and the rest over 120 min -->
   <!--  11/23/2002 changed amount given -->
   <config>
      <diam>19.98</diam>     <!--  monoject -->
      <syringe_name>20 cc monoject syringe</syringe_name>
      <dose_warning>40</dose_warning>     <!--  greater than  this dose assayed yields warning -->
      <minvol_warning>12</minvol_warning>     <!--  get warning if entered vol is  less than  this -->
      <maxvol_warning>20</maxvol_warning>     <!--  get warning if entered vol is  greater than  this -->
      <dsdef>.6</dsdef>
      <kbol>90</kbol>
      <tbolus>1</tbolus>
      <tdef>150</tdef>
      <tmin>90</tmin>
      <tmax>140</tmax>
      <desired_def>9.33</desired_def>
      <desired_warning>20</desired_warning>     <!--  greater than  this dose requested yields warning -->
      <inject_all_dose>1</inject_all_dose>     <!-- permitted to inject all dose - for monkey only! -->
      <mass_check>0</mass_check>
      <rmax>15.5</rmax>     <!-- max speeed for this diameter -->
      <time_warning>20</time_warning>     <!--  greater than  this time difference between assay and data entry -->
      <dec>1</dec>
      <hl>20.38</hl>
   </config>
   <user>
      <room>nmgeroom2</room>
      <ds>.5</ds>
   </user>

</parms>
