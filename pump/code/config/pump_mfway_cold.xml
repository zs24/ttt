<?xml version="1.0" encoding="UTF-8" ?>
<parms>
   <!--  pump_mfway_cold.com drive file for mfway cold infusions -->
   <!--  -->
   <!-- diam=="20.4" -->
   <!-- ds=="1.0"     !2 interlink connectors -->
   <config>
      <diam>19.98</diam>  <!-- corrected 12/20/99 -->
      <syringe_name>20 cc monoject syringe</syringe_name>
      <dose_warning>15</dose_warning>     <!--  greater than  this dose assayed yields warning -->
      <minvol_warning>12</minvol_warning>     <!--  get warning if entered vol is  less than  this -->
      <maxvol_warning>20</maxvol_warning>     <!--  get warning if entered vol is  greater than  this -->
      <prompt_ds>1</prompt_ds>
      <kbol>-1</kbol>     <!-- key that this is a cold only injection -->
      <tbolus>1</tbolus>
      <tdef>180</tdef>
      <tmin>110</tmin>
      <tmax>300</tmax>
      <desired_def>-1</desired_def>     <!-- def=all -->
      <desired_warning>10</desired_warning>     <!--  greater than  this dose requested yields warning -->
      <inject_all_dose>1</inject_all_dose>     <!-- permitted to inject all dose - for monkey only! -->
      <rmax>15.5</rmax>     <!-- max speeed for this diameter -->
      <time_warning>20</time_warning>     <!--  greater than  this time difference between assay and data entry -->
      <dec>0</dec>     <!-- no decay -->
      <hl>1E30</hl>
   </config>
   <user>
      <room>nmgeroom2</room>
      <ds>0.5</ds>     <!-- interlink connector ? -->
   </user>

</parms>
